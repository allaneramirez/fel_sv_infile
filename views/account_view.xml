<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="view_move_form_fel_sv_infile">
            <field name="name">account.move.form.fel_sv_infile</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="fel_sv.view_move_form_fel_sv"/>
            <field name="arch" type="xml">
                <data>
                    <field name="numero_control" position="after">
                        <field name="pdf_fel_sv" widget="url" groups="l10n_sv.group_fel_restricted_fields_sv"/>
                    </field>
                </data>
            </field>
        </record>



        <record id="view_company_form_fel_sv_infile" model="ir.ui.view">
            <field name="name">res.company.form.fel_sv_infile</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="fel_sv.view_company_form_fel_sv"/>
            <field name="arch" type="xml">
                <data>
                    <field name="certificador_fel_sv" position="before">
                        <field name="usuario_fel_sv" groups="l10n_sv.group_fel_restricted_fields_sv"/>
                        <field name="llave_fel_sv" groups="l10n_sv.group_fel_restricted_fields_sv"/>
                        <field name="pruebas_fel_sv" groups="l10n_sv.group_fel_restricted_fields_sv"/>
                    </field>
                </data>
            </field>
        </record>

    </data>
</odoo>
